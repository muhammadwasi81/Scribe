{
    "bail": 1,
    "verbose": true,
    "moduleFileExtensions": ["js", "jsx", "json", "node"],
    "transform": {
        "^.+\\.jsx?$": "babel-jest"
    },
    "moduleNameMapper": {
        "\\.(css|less|scss|sss|styl)$": "identity-obj-proxy"
    },
   
    "testMatch": [
        "**/__tests__/**/*.js?(x)",
        "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/dist/"
    ],
    
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
        "json",
        "lcov",
        "text",
        "clover"
    ],
    "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/dist/"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 80,
            "functions": 80,
            "lines": 80,
            "statements": 80
        }
    }


}
